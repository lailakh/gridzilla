        .col-lg-12.col-md-12
          iframe(src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d867278.5748069896!2d34.33162213499214!3d31.884665997251055!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x151cf2d28866bdd9%3A0xee17a001d166f686!2sPalestine!5e0!3m2!1sen!2s!4v1545245376242', width='100%', height='500px', frameborder='0', style='border:0', allowfullscreen='')
    // ==============container ======================
    footer
      .container
        .row
          .col-lg-6.col-md-12
            p
              i.fa.fa-twitter.btn.btn-info
              i.fa.fa-twitter.btn.btn-warning
              i.fa.fa-facebook.btn.btn-warning
              i.fa.fa-facebook.btn.btn-danger
          .col-lg-6.col-md-12
            p
              strong Best PSD Freebies  |  info@bestpsdfreebies.com
            p Copyright 2012 Gridzilla Theme  www.bestpsdfreebies.com
